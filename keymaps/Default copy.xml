<keymap version="1" name="Default copy" parent="$default">
  <action id="ActivatePythonConsoleToolWindow">
    <keyboard-shortcut first-keystroke="alt c" />
  </action>
  <action id="ReformatCode">
    <keyboard-shortcut first-keystroke="ctrl alt l" />
    <keyboard-shortcut first-keystroke="shift d" />
  </action>
</keymap>